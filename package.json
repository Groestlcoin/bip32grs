{
  "name": "bip32grs",
  "version": "1.0.2",
  "description": "A BIP32 compatible library",
  "keywords": [
    "bip32grs",
    "groestlcoinjs",
    "groestlcoin",
    "secp256k1"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "coverage-report": "nyc report --reporter=lcov",
    "coverage": "nyc --check-coverage --branches 90 --functions 90 npm run unit",
    "standard": "standard",
    "test": "npm run standard && npm run coverage",
    "unit": "tape test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/groestlcoin/bip32grs.git"
  },
  "dependencies": {
    "bs58grscheck": "^https://github.com/Groestlcoin/bs58grscheck.git",
    "create-hash": "^1.2.0",
    "create-hmac": "^1.1.7",
    "tiny-secp256k1": "^1.0.0",
    "typeforce": "^1.11.5",
    "wifgrs": "https://github.com/Groestlcoin/wifgrs.git"
  },
  "devDependencies": {
    "nyc": "^11.7.1",
    "standard": "^11.0.1",
    "tape": "^4.9.0"
  },
  "author": "Daniel Cousens",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bitcoinjs/bip32/issues"
  }
}
